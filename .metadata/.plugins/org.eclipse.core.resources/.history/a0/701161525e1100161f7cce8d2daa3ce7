package source;

import java.util.HashMap;
import java.util.Map;

public class SequenceFinder {

	public boolean oneWordSequence(String seqToFind)
	{
		boolean isSeq = false;
		
		for(int i = 0; i < seqToFind.length() - 6; ++i)
		{
			Map<Integer, Character> sequenceHolder = new HashMap<Integer, Character>();
			
			for(int j = i; j < i + 6; ++j)
			{
				if(j < i + 3)
				{
					if(!sequenceHolder.containsKey(seqToFind.charAt(j)))
					{
						sequenceHolder.put(seqToFind.charAt(j), j);
					}
					else
					{
						break;
					}
				}
				else
				{
					
				}
			}
		}
	}
	
}
